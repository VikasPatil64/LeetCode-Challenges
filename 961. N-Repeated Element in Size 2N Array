# https://leetcode.com/problems/n-repeated-element-in-size-2n-array/description/?envType=problem-list-v2&envId=array

class Solution:
    def repeatedNTimes(self, nums: List[int]) -> int:
        lst = []
        for i in nums:
            if i in lst:
                return i
            else:
                lst.append(i)
