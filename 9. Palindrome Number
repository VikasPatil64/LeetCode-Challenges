#https://leetcode.com/problems/palindrome-number/description/

class Solution:
    def isPalindrome(self, x: int) -> bool:
        strNum = str(x)
        i = 0
        j = len(strNum) - 1
        while i < j:
            if strNum[i] != strNum[j]:
                return False
            i += 1
            j -= 1
        return True

            
        
